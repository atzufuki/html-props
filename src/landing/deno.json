{
  "imports": {
    "@html-props/core/**/*": "../../core",
    "@html-props/signals/**/*": "../../signals",
    "marked": "npm:marked@^17.0.1"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "deno.window",
      "DOM.Iterable",
      "DOM.AsyncIterable"
    ],
    "strict": true,
    "strictFunctionTypes": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": false
  }
}
